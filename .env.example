# Копируйте в .env и заполните значения

# База данных (после внедрения БД)
# DATABASE_URL=postgresql://user:password@localhost:5432/ventmash
# или для SQLite:
# DATABASE_URL=sqlite:///ventmash.db

# Сервер
PORT=5000

# Яндекс.Метрика (ID счётчика)
# YANDEX_METRIKA_ID=XXXXXXXX

# Секрет для сессий (если понадобится)
# SECRET_KEY=your-secret-key
